  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment.line",
        "comment.block",
        "comment.block.documentation",
        "punctuation.definition.comment",
        "meta.toc-list"
      ],
      "settings": {
        "foreground": "#919191a3",
        "fontStyle": "italic"
      }
    },

    {
      "name": "Constant",
      "scope": [
        "constant.numeric",
        "constant.numeric.integer",
        "constant.numeric.integer.other",
        "constant.numeric.float",
        "constant.numeric.float.other",
        "constant.numeric.complex",
        "constant.numeric.complex.real",
        "constant.numeric.complex.imaginary",
        "constant.language",
        "onstant.character.escape",
        "constant.other.placeholder",
        "constant.other.caps",
        "punctuation.separator.annotation.result"
      ],
      "settings": {
        "foreground": "#e394dc"
      }
    },

    {
      "name": "Entity",
      "scope": [
        "entity.name.class",
        "entity.name.struct",
        "entity.name.enum",
        "entity.name.union",
        "entity.name.trait",
        "entity.name.interface",
        "entity.name.impl",
        "entity.name.type",
        "entity.name.class.forward-decl",
        "entity.name.function",
        "entity.name.function.constructor",
        "entity.name.function.destructor",
        "entity.name.namespace",
        "entity.name.constant",
        "entity.name.label"
      ],
      "settings": {
        "foreground": "#b588dc"
      }
    },
    {
      "name": "Inheritance",
      "scope": ["entity.other.inherited-class"],
      "settings": {
        "foreground": "#b588dc",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid.illegal", "invalid.deprecated"],
      "settings": {
        "foreground": "#25c9b6"
      }
    },

    {
      "name": "Keyword and Function",
      "scope": [
        "keyword.control",
        "keyword.control.conditional",
        "keyword.control.import",
        "storage.type.function",
        "punctuation.definition.keyword",
        "keyword.other",
        "variable.language.this",
        "variable.language.super",
        "support.type"
      ],
      "settings": {
        "foreground": "#ff8585"
      }
    },

    {
      "name": "Operator",
      "scope": [
        "keyword.operator",
        "keyword.operator.assignment",
        "keyword.operator.arithmetic",
        "keyword.operator.bitwise",
        "keyword.operator.logical",
        "keyword.operator.word"
      ],
      "settings": {
        "foreground": "#9cdcfe"
      }
    },

    {
      "name": "Meta",
      "scope": [
        "meta.class",
        "meta.struct",
        "meta.enum",
        "meta.union",
        "meta.trait",
        "meta.interface",
        "meta.impl",
        "meta.type",
        "meta.function.return-type",
        "meta.namespace",
        "meta.preprocessor",
        "meta.annotation",
        "meta.annotation.identifier",
        "punctuation.definition.annotation",
        "meta.path",
        "meta.function-call",
        "meta.braces",
        "meta.attribute",
        "meta.member.access",
        "variable.other.object"
      ],
      "settings": {
        "foreground": "#cfaa55"
        // "fontStyle": "bold" ebc877
      }
    },
    {
      "name": "Expression",
      "scope": ["meta.function.expression"],
      "settings": {
        "foreground": "#dedede"
      }
    },
    {
      "name": "Parameter",
      "scope": [
        "support.variable.property",
        "meta.parameters",
        "punctuation.definition.parameters",
        "meta.annotation.parameters",
        "meta.function-call.arguments",
        "variable.other.property",
        "meta.function.parameters"
      ],
      "settings": {
        "foreground": "#5ba89e"
      }
    },

    {
      "name": "Variable",
      "scope": [
        "variable.other",
        "punctuation.definition.variable",
        "variable.other.constant",
        "variable.language",
        "variable.parameter",
        "variable.other.member",
        "variable.function",
        "variable.annotation"
      ],
      "settings": {
        "foreground": "#c7c3c3"
      }
    },
    {
      "name": "Storage",
      "scope": [
        "storage.type",
        "storage.modifier",
        "storage.type.function keyword.declaration.function"
      ],
      "settings": {
        "foreground": "#ff8585"
      }
    },
    {
      "name": "String and Block",
      "scope": [
        "string.quoted.docstring.multi",
        "string.quoted.triple",
        "string.quoted.double",
        "string.quoted.other",
        "string.template",
        "String"
      ],
      "settings": {
        "foreground": "#8ebee9"
      }
    },

    // CSS Support
    {
      "name": "CSS Class and Support",
      "scope": [
        "source.css support.type.property-name",
        "source.sass support.type.property-name",
        "source.scss support.type.property-name",
        "source.less support.type.property-name",
        "source.stylus support.type.property-name",
        "source.postcss support.type.property-name",
        "source.css meta.property-name.css"
      ],
      "settings": {
        "foreground": "#82d2ce"
      }
    },
    {
      "name": "CSS Propertyies Values",
      "scope": ["meta.property-value.css", "keyword.control.at-rule.media.css"],
      "settings": {
        "foreground": "#e394dc"
      }
    },
    {
      "name": "CSS Classes",
      "scope": [
        "entity.other.attribute-name.class",
        "entity.name.tag.css",
        "meta.selector.css"
      ],
      "settings": {
        "foreground": "#af9cff"
      }
    },
    {
      "name": "CSS ID's",
      "scope": ["source.sass keyword.control"],
      "settings": {
        "foreground": "#82AAFF"
      }
    },
    {
      "name": "URL",
      "scope": ["*url*", "*link*", "*uri*"],
      "settings": {
        "fontStyle": "underline"
      }
    },
    // JS support
    {
      "name": "Sub-methods",
      "scope": [
        "entity.name.module.js",
        "variable.import.parameter.js",
        "variable.other.class.js"
      ],
      "settings": {
